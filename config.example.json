{
	"socket": {
		"path": "/tmp/ircbot.sock"
	},
	"irc": {
		"server": "irc.example.com",
		"nickname": "notifier",
		"options": {
			"autoConnect": true,
			"channels": [
				"#example"
			],
			"debug": true,
			"port": 6697,
			"realName": "IRC bot",
			"secure": true,
			"showErrors": true,
			"userName": "ircbot"
		},
		"announceChannel": "#example"
	}
	"process": {
		"startupScript": "/home/user/ircBot/start.sh"
	},
	"http": {
		"listen": {
			"host": "::",
			"port": 1234
		}
	}
}
